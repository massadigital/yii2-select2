{
    "name": "massadigital/yii2-select2",
    "description": "Widget select2 yii2",
    "keywords": ["yii2", "select2"],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Carlos"
        }
    ],
    "require": {
        "bower-asset/select2": "*",
        "bower-asset/select2-bootstrap-theme": "*"
    },
    "extra": {
        "asset-repositories": [
            {
                "type": "bower-git",
                "url": "git://github.com/fk/select2-bootstrap-theme.git"
            }
        ]
    },
    "autoload": {
        "psr-4": {
            "massadigital\\select2\\": ""
        }
    }
}
